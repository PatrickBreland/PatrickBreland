CACV = if(fConsolidated[Fee Type] = "SaaS",
fConsolidated[ACV]*2,


if(fConsolidated[Fee Type] = "Perpetual",fConsolidated[AmountGBP (Excl Zero)]*0.33,

if(fConsolidated[Fee Type] = "Support",fConsolidated[ACV]*0.33,

if(fConsolidated[Fee Type] = "Rentals",
fConsolidated[ACV]*1.5,

if(fConsolidated[Fee Type] = "Services",
if(fConsolidated[Service_Margin]<20,fConsolidated[ACV]*0.2,
if(fConsolidated[Service_Margin]>=20,fConsolidated[ACV]*0.33)),

fConsolidated[ACV])

))))
